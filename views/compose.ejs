<%- include('partials/header.ejs') -%>


<h1>Compose</h1>



<form action="/compose" method="POST">
  <div class="form-group">
    <label for="exampleInputEmail1">Title</label>
    <input type="text" class="form-control" name="postTitle"  aria-describedby="emailHelp" placeholder="Post Title">
    
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Post</label>
    <textarea type="text" name="postEntry" class="form-control" cols="60"  placeholder="Post Entry"></textarea>
  </div>
  <br>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>







